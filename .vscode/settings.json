    ["addCls\\(([^\\)]*)\\)", "`(.*?)`"],
    ["\\$clsFunc`([\\s\\S]*?)`", "(\\S+)"],
    ["\\$clsFunc\\(([^\\)]*)\\)", "\\\"(.*?)\\\""],
    ["\\$clsStr`([\\s\\S]*?)`", "(\\S+)"],
    ["\\$clsStr\\(([^\\)]*)\\)", "\\\"(.*?)\\\""]
  ]
}
